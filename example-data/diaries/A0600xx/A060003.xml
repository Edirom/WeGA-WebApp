<ab xmlns="http://www.tei-c.org/ns/1.0" n="1817-01-03" xml:id="A060003" status="approved">
    <date when="1817-01-03">d: 3<hi rend="superscript">t</hi></date>
    <seg type="correspondence" subtype="sender">an <persName key="A000213">Lina</persName> geschrieben. <hi rend="underline" n="1">an <rs type="persons" key="A000114 A000111">Beers</rs></hi>.</seg>     
    <seg>von <persName key="A001693">Schlesinger</persName> die <measure type="income" unit="fd" quantity="12">12 <hi rend="latintype">Fried: dor</hi></measure> <lb/>  
        für <persName key="A000584">Gänsbacher</persName> zurük erhalten.</seg>
    <seg>an alten Fuhren <measure type="expense" unit="rh" quantity="1">1 rh</measure> <measure type="expense" unit="gr" quantity="16">16</measure></seg> <lb/>
    <seg type="meeting" xml:id="meeting01">Mittag bey <hi rend="latintype"><persName key="A002434">Weiss</persName></hi>. Droschke hin <add place="above">mit beiden 
        <hi rend="latintype"><rs type="persons" key="A001600 A002390">Rombergs</rs></hi>.</add> <measure type="expense" unit="gr" quantity="6">6</measure></seg> <lb/>
    <seg type="meeting" xml:id="meeting02">von da zu <rs type="persons" key="A001611">Rosenstiels</rs>, Droschke und Trinkgeld, den Wagen <measure type="expense" unit="gr" quantity="18">18.</measure> <lb/>
        bis 12 Uhr.</seg>
    <seg type="chamberMusic" corresp="#meeting01 #meeting02">viel gespielt an beiden Orten.</seg>
